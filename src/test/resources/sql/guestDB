
DROP TABLE IF EXISTS JC_GUEST;

CREATE TABLE JC_GUEST
(
        GUEST_ID SERIAL,
        FIRST_NAME VARCHAR(50) NOT NULL,
        LAST_NAME VARCHAR(50) NOT NULL,
        PHONE VARCHAR(50) NOT NULL,
        EMAIL VARCHAR(50) NOT NULL,
	    ROOM SERIAL NOT NULL,
        PRIMARY KEY (GUEST_ID)
);

INSERT INTO JC_GUEST (FIRST_NAME, LAST_NAME, PHONE, EMAIL,ROOM) VALUES ('Peter','Belgy','+79112345678','peter@pisem.net',105);
INSERT INTO JC_GUEST (FIRST_NAME, LAST_NAME, PHONE, EMAIL,ROOM) VALUES ('Helga','Forte','+79118765432','helga@pisem.net',103);

SELECT * from JC_GUEST;